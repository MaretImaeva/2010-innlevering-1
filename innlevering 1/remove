
def FindMin(x):
    if x.left is None:
        return x
    return FindMin(x.left)


def Remove(v,x):
    if v is None:
        return None
    elif x <v.element:
        v.left = Remove(v.left,x)
        return v
    elif x>v.element:
        v.right = Remove(v.right,x)
        return v
    elif v.left is None:
        return v.left
    u = FindMin(v.right)
    v.element = u.element
    v.right = Remove(v.right, u.element)
    return v



root = None         # start med tomt tre
root = insert(root,9)
root = insert(root,3)
root = insert(root, 1)  
root = insert(root, 2)  
root = insert(root,11)


print(root.element)        
print(root.right.element) 
print(root.left.element) 
