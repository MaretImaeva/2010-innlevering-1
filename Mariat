def input_fra_bruker():
    antall_operasjoner = int(input("Skriv et heltall: "))
    while antall_operasjoner > 0:
        bruker_input = input("Venter kommando:\n").split(" ")
        kommando = bruker_input[0]
        x = int(bruker_input[1])
        if kommando == "contains":
            return contains(x)
        if kommando == "insert":
            return insert(x)
        if kommando == "remove":
            return remove(x)
        if kommando == "size":
            
        
input_fra_bruker()





